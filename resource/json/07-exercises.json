  "practiceQuestions": [
    {
      "question": "What are the six data types supported by JSON?",
      "answer": "String, Number, Boolean, null, Object, Array"
    },
    {
      "question": "Can JSON contain comments?",
      "answer": "No, JSON does not support comments"
    },
    {
      "question": "What's the difference between JSON and JavaScript objects?",
      "answer": "JSON requires double quotes for keys, doesn't support functions or undefined values, and is text-based. JavaScript objects can have unquoted keys, support functions, and are native objects."
    },
    {
      "question": "How do you handle special characters in JSON strings?",
      "answer": "Use escape sequences: \\\" for quotes, \\\\ for backslash, \\n for newline, \\t for tab, etc."
    },
    {
      "question": "Can JSON keys be duplicated?",
      "answer": "While JSON parsers may accept duplicate keys, it's not recommended as behavior is undefined. Only the last value is typically kept."
    }
  ],

  "commonMistakes": [
    {
      "mistake": "Using single quotes instead of double quotes",
      "wrong": "{'name': 'John'}",
      "correct": "{\"name\": \"John\"}"
    },
    {
      "mistake": "Trailing commas",
      "wrong": "{\"name\": \"John\", \"age\": 30,}",
      "correct": "{\"name\": \"John\", \"age\": 30}"
    },
    {
      "mistake": "Using undefined values",
      "wrong": "{\"value\": undefined}",
      "correct": "{\"value\": null}"
    },
    {
      "mistake": "Including functions",
      "wrong": "{\"getName\": function() { return 'John'; }}",
      "correct": "Functions are not allowed in JSON"
    }
  ]
